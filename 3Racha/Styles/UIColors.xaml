    <SolidColorBrush x:Key="titleColor2" Color="#D5CFF5"/>
    <SolidColorBrush x:Key="titleColor3" Color="#2BA176"/>
    <SolidColorBrush x:Key="titleColor4" Color="#2D5638"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#2BA176"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#F88D9E"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#F88D9E"/>

    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#fbbec8"/>
